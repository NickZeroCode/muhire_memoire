{
    "builds": [{
        "src": "muhire_memoire/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", 
                    "runtime": "python3.9",
                    "buildCommand": "bash build_files.sh" 
                }
                  }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "muhire_memoire/wsgi.py"
        },
        {
            "src": "/static(.*)",
            "dest": "/static/$1"
        }
    ]
}